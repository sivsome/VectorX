DeleteGui.Name = "Delete Gui"
DeleteGui.Parent = Miscs
DeleteGui.BackgroundColor3 = Color3.fromRGB(97, 58, 11)
DeleteGui.BorderSizePixel = 0
DeleteGui.Position = UDim2.new(0, 1, 0, 54)
DeleteGui.Size = UDim2.new(0, 148, 0, 25)
DeleteGui.Font = Enum.Font.Cartoon
DeleteGui.Text = "Delete Gui"
DeleteGui.TextColor3 = Color3.fromRGB(0, 0, 0)
DeleteGui.TextSize = 16.000


AutoFarmTittle.MouseButton1Down:connect(function()
	if AutoFarm.Size == UDim2.new(0, 150, 0, 55) then
		AutoFarm.Size = UDim2.new(0, 150, 0, 25)
		AutoMallows.Visible = false
	else
		AutoFarm.Size = UDim2.new(0, 150, 0, 55)
		AutoMallows.Visible = true
	end
end)

AutoMallows.MouseButton1Down:connect(function()
	if AutoMallows.TextColor3 == Color3.fromRGB(0, 0, 0) then
		local playerss = game:GetService("Players").LocalPlayer.Name
		local Event = game:GetService("Workspace").PlayerCampingItems[playerss].MallowMachine.MallowScript.RetrieveEvent
		local Time = 1
		AutoMallows.TextColor3 = Color3.fromRGB(0, 160, 0)
		while true do
			wait(Time)
			Event:FireServer()
		end
	end
end)

CreditsTittle.MouseButton1Down:connect(function()
	if Credits.Size == UDim2.new(0, 150, 0, 80) then
		Credits.Size = UDim2.new(0, 150, 0, 25)
		Credits1.Visible = false
		Credits2.Visible = false
	else
		Credits.Size = UDim2.new(0, 150, 0, 80)
		Credits1.Visible = true
		Credits2.Visible = true
	end
end)

MiscsTittle.MouseButton1Down:connect(function()
	if Miscs.Size == UDim2.new(0, 150, 0, 80) then
		Miscs.Size = UDim2.new(0, 150, 0, 25)
		AntiAfk.Visible = false
		DeleteGui.Visible = false
	else
		Miscs.Size = UDim2.new(0, 150, 0, 80)
		AntiAfk.Visible = true
		DeleteGui.Visible = true
	end
end)

AntiAfk.MouseButton1Down:connect(function()
	if AntiAfk.TextColor3 == Color3.fromRGB(0, 0, 0) then
		loadstring(game:HttpGet("https://pastebin.com/raw/L2E6ft9L", true))()
		AntiAfk.TextColor3 = Color3.fromRGB(0, 160, 0)
	end
end)

DeleteGui.MouseButton1Down:connect(function()
	game:GetService("CoreGui"):WaitForChild("Backpacking Beta"):Remove()
end)
