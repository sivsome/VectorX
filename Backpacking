
-- Gui to Lua
-- Version: 3.1

-- Instances:

local BackpackingBeta = Instance.new("ScreenGui")
local AutoFarm = Instance.new("Frame")
local AutoFarmTittle = Instance.new("TextButton")
local AutoMallows = Instance.new("TextButton")
local Credits = Instance.new("Frame")
local CreditsTittle = Instance.new("TextButton")
local Credits1 = Instance.new("TextLabel")
local Credits2 = Instance.new("TextLabel")
local Miscs = Instance.new("Frame")
local MiscsTittle = Instance.new("TextButton")
local AntiAfk = Instance.new("TextButton")
local DeleteGui = Instance.new("TextButton")

--Properties:

BackpackingBeta.Name = "Backpacking Beta"
BackpackingBeta.Parent = game.CoreGui

AutoFarm.Name = "AutoFarm"
AutoFarm.Parent = BackpackingBeta
AutoFarm.Active = true
AutoFarm.BackgroundColor3 = Color3.fromRGB(97, 58, 11)
AutoFarm.BorderColor3 = Color3.fromRGB(226, 154, 11)
AutoFarm.BorderSizePixel = 3
AutoFarm.Position = UDim2.new(0, 14, 0, 20)
AutoFarm.Size = UDim2.new(0, 150, 0, 55)
AutoFarm.Draggable = true

AutoFarmTittle.Name = "AutoFarm Tittle"
AutoFarmTittle.Parent = AutoFarm
AutoFarmTittle.BackgroundColor3 = Color3.fromRGB(97, 58, 11)
AutoFarmTittle.BorderColor3 = Color3.fromRGB(226, 154, 11)
AutoFarmTittle.BorderSizePixel = 3
AutoFarmTittle.Size = UDim2.new(0, 150, 0, 25)
AutoFarmTittle.Font = Enum.Font.Cartoon
AutoFarmTittle.Text = "AutoFarm"
AutoFarmTittle.TextColor3 = Color3.fromRGB(0, 0, 0)
AutoFarmTittle.TextScaled = true
AutoFarmTittle.TextSize = 14.000
AutoFarmTittle.TextWrapped = true

