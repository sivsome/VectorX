AutoMallows.Parent = AutoFarm
AutoMallows.BackgroundColor3 = Color3.fromRGB(97, 58, 11)
AutoMallows.BorderSizePixel = 0
AutoMallows.Position = UDim2.new(0, 1, 0, 29)
AutoMallows.Size = UDim2.new(0, 148, 0, 25)
AutoMallows.Font = Enum.Font.Cartoon
AutoMallows.Text = "Auto Mallows"
AutoMallows.TextColor3 = Color3.fromRGB(0, 0, 0)
AutoMallows.TextSize = 16.000

